/*页面的整体布局*/
	#header,#nav_1,#nav_2,#banner,#section,#footer1,#footer2,#nav_2{
		width:1349px;
		margin:0 auto;
	}
/*header部分*/
/*设置艺龙图标*/
	#header{
	box-sizing:border-box;
	padding:10px 60px;
	}
	#top_main>p{
		float:left;
		width:116px;
		height:50px;
		background:url(../image/1.png) no-repeat -660px 0px;
}


/*设置页头右边部分*/
#top_right{
float:right;
}
#top_right>ul>li,#top_right>ul{
display:inline-block;
}
#top_right>ul>li{
text-align:center;
width:80px;
line-height:30px;
position:relative;
top:-10px;
}

#top_right>ul>li b{
border-right:1px solid #aaa;
margin-left:15px;
}
#top_right>ul>li.english{
border:1px solid transparent;
border-bottom:none;
/*配合弹出框做绝对定位*/
position:relative;
}
/*enlish弹出框*/
#english_items{
position:absolute;
border:1px solid #aaa;
border-top:none;
width:70px;
padding:0px 5px;
top:25px;
left:-1px;
background:#ccffff;
z-index:100;
/**/
display:none;
}
#english_items>li{
border-top:1px dashed #aaa;

}

#top_right>ul>li.english:hover{
border-color:#aaa;
background:#ccffff;
}
/*两个向下的实心箭头*/
#top_right>ul>li.english>s{
display:inline-block;
width:0px;
height:0px;
border:5px solid transparent;
border-top-color:#aaa;
position:relative;
top:3px;
}
/*手机版专享*/
#count{
display:inline-block;
margin:0px 20px;
/*相对定位配合绝对定位*/
position:relative;
}
#count>div,#tel>div{
display:inline-block;
margin-left:20px;
font-size:14px;
}
#count>div>p,#tel>div>p{
line-height:30px;
}
#count>span{
padding:20px;
background:url(../image/1.png) no-repeat -2px -763px;
position:relative;
top:0px;
left:0px;
}
/*手机专享弹出框*/
#count_items{
border:1px solid #ddd;
width:228px;
height:103px;
background:#fff url(../image/4.png) no-repeat -380px -232px;
position:absolute;
z-index:100;
top:60px;
left:10px;
/**/
display:none;
}
#count_items i,#tel_items>i{
padding:5px 9px;
background:url(../image/4.png) no-repeat -212px -2px;
position:absolute;
top:-8px;
left:35px;
}
/*客服电话*/
#tel{
display:inline-block;
position:relative;
}
#tel>span{
padding:20px;
background:url(../image/1.png) no-repeat -43px -763px;
}

#tel>div>b{
display:inline-block;
width:0px;
height:0px;
border:5px solid transparent;
border-top-color:#6666ff;;
position:relative;
top:3px;
}
/*客服电话弹出框*/
#tel_items{
border:1px solid #ddd;
padding:5px 10px;
position:absolute;
z-index:100;
width:170px;
top:60px;
left:-10px;
background:#fff;
/**/
display:none;
}
#tel_items>ul>li{
border-bottom:1px dashed #ddd;
text-align:left;
line-height:30px;
}
/*footer1部分*/
#service_info{
	width:1200px;
	background:#f7f7f7;
	height:80px;
	padding:20px;
	margin:0 auto;
	border-bottom:1px solid #e5e5e5;
}
/*footer1上部分*/

#service_info ul li,#service_info ul li b,#service_info ul li h3{
	display:inline-block;
}

#service_info ul li b{
	width:68px;
	height:68px;
	margin-right:40px;
}
#service_info ul li.range b{
background:url(../image/3.png) no-repeat -90px 2px;
}
#service_info ul li.activities b{
background:url(../image/3.png) no-repeat -155px 2px;
}
#service_info ul li.internation b{
background:url(../image/3.png) no-repeat -225px 2px;
}
#service_info ul li.service b{
background:url(../image/3.png) no-repeat -293px 2px;
}
#service_info ul li h3{
	position:relative;
	top:-30px;
	margin-right:70px;
}
#message{
margin:0 auto;
height:80px;
width:1240px;

background:#f7f7f7;
}
#message .box_email{
width:110px;
height:80px;
background:url(../image/3.png) no-repeat -370px -9px;
